  "version": "0.2.0",
  "configurations": [
    {
      "name": "graph sine",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoom}/bin/graph",
      "args": ["<", "${workspaceRoom}/sine.bin"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": true,
      "console": "externalTerminal",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}